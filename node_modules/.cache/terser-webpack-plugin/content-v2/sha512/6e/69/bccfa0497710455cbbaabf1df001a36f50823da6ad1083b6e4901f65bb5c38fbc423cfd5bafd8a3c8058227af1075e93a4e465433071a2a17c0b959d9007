points\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"activities__points__name\",children:\"Swimming\"})})]})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"Reviews\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"reviews__title\",children:[comment.length,\" Reviews\"]}),comment.map(function(data,index){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"reviews__box\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"img\",{className:\"reviewer__img\",alt:\"reviewer\",src:data.author.photoURL}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"reviewer__info\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"reviewer__stars\",children:[\"\\u2605\",data.rating]}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"reviewer__name\",children:data.author.email}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"reviewer__comment\",children:data.review}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"reviewer__date\",children:data.createdAt.toDate().toLocaleDateString()})]})]},index);})]}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"porduct__detail__map\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(MapProductDetail_App,{})})]})})});}/* harmony default export */ var components_RoomDetail = (RoomDetail_RoomDetail);\n// CONCATENATED MODULE: ./src/components/pages/ProductDetail.js\nfunction ProductDetail(){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(jsx_runtime[\"Fragment\"],{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(components_Navbar,{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(components_RoomDetail,{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(components_Footer,{})]});}/* harmony default export */ var pages_ProductDetail = (ProductDetail);\n// EXTERNAL MODULE: ./src/components/AddCamp.css\nvar AddCamp = __webpack_require__(105);\n\n// EXTERNAL MODULE: ./node_