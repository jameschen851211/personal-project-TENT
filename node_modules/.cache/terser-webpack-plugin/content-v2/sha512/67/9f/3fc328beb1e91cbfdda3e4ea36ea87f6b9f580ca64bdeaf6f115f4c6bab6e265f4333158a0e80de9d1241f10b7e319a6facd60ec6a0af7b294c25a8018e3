\n  const { likes, dislikes, active } = state;\n\n  switch (action.type) {\n    case HANDLE_LIKE:\n      return {\n        ...state,\n        likes: state.likes + 1,\n        dislikes: active === \"dislike\" ? dislikes - 1 : dislikes,\n        active: \"like\",\n      };\n    case HANDLE_DISLIKE:\n      return {\n        ...state,\n        likes: active === \"like\" ? likes - 1 : likes,\n        active: \"dislike\",\n        dislikes: dislikes + 1,\n      };\n    default:\n      return state;\n  }\n};\n\nconst LikeOrDislike = () => {\n  const [state, dispatch] = useReducer(reducer, initialState);\n  const { likes, dislikes, active } = state;\n  return (\n    <div style={{ display: \"flex\" }}>\n      <button\n        style={{\n          color: active === \"like\" ? \"green\" : \"black\",\n          marginRight: \"20px\",\n          width: \"40px\",\n          height: \"40px\",\n          fontSize: \"30px\",\n        }}\n        onClick={() =>\n          active !== \"like\" ? dispatch({ type: HANDLE_LIKE }) : null\n        }\n      >\n        <strong></strong>\n        {/* &nbsp;|&nbsp;\n        {likes} */}\n      </button>\n      <button\n        style={{\n          color: active === \"dislike\" ? \"red\" : \"black\",\n          width: \"40px\",\n          heigth: \"40px\",\n          fontSize: \"30px\",\n        }}\n        onClick={() =>\n          active !== \"dislike\" ? dispatch({ type: HANDLE_DISLIKE }) : null\n        }\n      >\n        <strong></strong>\n        {/* &nbsp;|&nbsp;\n        {dislikes} */}\n      </button>\n    </div>\n  );\n};\n\nexport default LikeOrDislike;\n","import React from \"react\";\nimport Popup from \"reactjs-popup\";\nimport \"./PopupButton.css\";\nimport StarRating from \"./StarRating\";\nimport LikeOrDislike from \"./LikeOrDislike\";\n\nfunction PopupButton() {\n  return (\n    <Popup\n      trigger={\n        <button className=\"trips__cards__items__button\"> Evaluate </button>\n      }\n      modal\n      nested\n    >\n      