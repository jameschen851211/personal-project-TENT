    <button\n        style={{\n          color: active === \"like\" ? \"green\" : \"black\",\n          marginRight: \"20px\",\n          width: \"40px\",\n          height: \"40px\",\n          fontSize: \"30px\",\n        }}\n        onClick={() =>\n          active !== \"like\" ? dispatch({ type: HANDLE_LIKE }) : null\n        }\n      >\n        <strong>✔︎</strong>\n        {/* &nbsp;|&nbsp;\n        {likes} */}\n      </button>\n      <button\n        style={{\n          color: active === \"dislike\" ? \"red\" : \"black\",\n          width: \"40px\",\n          heigth: \"40px\",\n          fontSize: \"30px\",\n        }}\n        onClick={() =>\n          active !== \"dislike\" ? dispatch({ type: HANDLE_DISLIKE }) : null\n        }\n      >\n        <strong>✖︎</strong>\n        {/* &nbsp;|&nbsp;\n        {dislikes} */}\n      </button>\n    </div>\n  );\n};\n\nexport default LikeOrDislike;\n","import React from \"react\";\nimport Popup from \"reactjs-popup\";\nimport \"./PopupButton.css\";\nimport StarRating from \"./StarRating\";\nimport LikeOrDislike from \"./LikeOrDislike\";\n\nfunction PopupButton() {\n  return (\n    <Popup\n      trigger={\n        <button className=\"trips__cards__items__button\"> Evaluate </button>\n      }\n      modal\n      nested\n    >\n      {(close) => (\n        <div className=\"modal\">\n          <button className=\"close\" onClick={close}>\n            &times;\n          </button>\n          <div className=\"header\"> Evaluate </div>\n          <div className=\"content\">\n            {\" \"}\n            <div className=\"content-input\">\n              Stars:\n              <StarRating />\n            </div>\n            <br />\n            <div className=\"content-input\">\n              Link/Dislike:\n              <LikeOrDislike />\n            </div>\n            <br />\n            <div className=\"content-input\">\n              Reviews:<input className=\"popupInput\"></input>\n            </div>\n          </div>\n   